    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "3.16.0",
    "@remix-run/dev": "1.7.0",
    "@remix-run/eslint-config": "1.7.0",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "eslint": "8.23.0",
    "miniflare": "2.7.1",
    "npm-run-all": "4.1.5",
    "typescript": "4.9.5",
    "wrangler": "2.0.28"
  },
